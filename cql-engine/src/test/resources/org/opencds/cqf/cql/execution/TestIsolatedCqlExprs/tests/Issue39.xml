<?xml version="1.0" encoding="utf-8"?>
<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://hl7.org/fhirpath/tests" xsi:schemaLocation="http://hl7.org/fhirpath/tests ../../testSchema/testSchema.xsd">
	<!-- TEST FILE WITH RESPECT TO: https://github.com/DBCG/cql_engine/issues/39 -->
	<group name="Issue39">
		<test name="Issue39">
			<expression>
				Interval[
					@2017-12-20T11:00:00,
					DateTime(
					year from (@2017-12-20T11:00:00 + 1 day),
					month from (@2017-12-20T11:00:00 + 1 day),
					day from (@2017-12-20T11:00:00 + 1 day),
					0, 0, 0, 0,
					timezone from (@2017-12-20T11:00:00 + 1 day)
					)
				) ~ Interval [ @2017-12-20T11:00:00, @2017-12-20T23:59:59.999 ]
			</expression>
			<output>true</output>
		</test>
	</group>
</tests>
